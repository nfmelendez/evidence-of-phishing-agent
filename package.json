{
  "name": "oz-uupsupgradeable-contract-agent",
  "version": "1.0.0",
  "description": "This agent detects when the UUPSUpgradeable contract is upgraded and then selfdestructed leaving the contract broken and funds locked.",
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec \"npm run build && forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "npm run build && forta-agent run --tx",
    "block": "npm run build && forta-agent run --block",
    "range": "npm run build && forta-agent run --range",
    "file": "npm run build && forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest",
    "mytx": "npm run build && forta-agent run --tx 0x5e4c7966b0eaddaf63f1c89fc1c4c84812905ea79c6bee9d2ada2d2e5afe1f34",
    "myrange": "npm run build && forta-agent run --range 13650638..13652198",
    "mytx3": "npm run build && forta-agent run --tx 0x5e4c7966b0eaddaf63f1c89fc1c4c84812905ea79c6bee9d2ada2d2e5afe1f34,0x5e4c7966b0eaddaf63f1c89fc1c4c84812905ea79c6bee9d2ada2d2e5afe1f34"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "forta-agent": "^0.0.33"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/nodemon": "^1.19.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
